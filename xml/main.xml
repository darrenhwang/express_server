<ds namespace="H" database="main">
    <table name="g_bw_menu" primary="type,func,target,v">
        <comment>黑白名单</comment>
        <int name="type" title="类型">
            <comment>黑、白</comment>
            <combo>
                <opt name="black" value="0">黑</opt>
                <opt name="white" value="1">白</opt>
            </combo>
        </int>
        <int name="func" title="功能">
            <combo>
                <opt name="login" value="0">登陆</opt>
                <opt name="chat" value="1">聊天</opt>
                <opt name="adv" value="2">广告</opt>
                <opt name="maintenance" value="3">维护</opt>
                <opt name="region" value="4">区服段</opt>
            </combo>
        </int>
        <int name="target" title="目标">
            <combo>
                <opt name="ip" value="0">IP</opt>
                <opt name="acc" value="1">账号</opt>
                <opt name="usr" value="2">用户</opt>
            </combo>
        </int>
        <varchar name="banReason" length="256" title="封号原因"/>
        <varchar name="v" length="128" title="目标值"/>
        <datetime name="invalidTime" title="失效时间"/>
    </table>

    <table name="g_acc_set" primary="aid,type" keys="uid">
        <comment>玩家账号设置表</comment>
        <int name="aid" title="玩家账号Id"/>
        <int name="type" title="设置类型">
            <combo>
                <opt name="phoneBind" value="1">手机绑定</opt>
            </combo>
        </int>
        <int name="uid" title="设置的玩家ID"/>
        <text name="value" type="any" title="设置值"/>
        <int name="isOpen" title="是否开启"/>
        <datetime name="recvTime" title="领取时间"/>
        <datetime name="uTime" title="更新时间"/>
        <datetime name="cTime" title="创建时间"/>
    </table>

    <table name="g_gs_fml_info" primary="fid" key-ignore="true" gen-types="server,gm">
        <comment>所有区服宗门信息</comment>
        <int name="fid" title="宗门ID"/>
        <varchar name="name" length="50" title="宗门名字"/>
        <int name="gsIdx" title="所在分服ID"/>
        <int name="cuid" title="创建者的玩家ID"/>
        <int name="mstUid" title="会长uid"/>
        <varchar name="mstName" length="50" title="会长名字"/>
        <varchar name="mstIco" length="500" title="会长头像"/>
        <int name="lvl" title="宗门等级"/>
        <varchar name="ico" length="32" title="家族旗帜"/>
        <int name="mbCnt" title="宗门成员数"/>
    </table>

    <table name="g_gs_usr_info" primary="uid" keys="accName;did" key-ignore="true" gen-types="server,gm">
        <comment>玩家所有区服角色信息</comment>
        <int name="uid" title="用户ID"/>
        <varchar name="did" length="16" title="玩家显示ID"/>
        <int name="aid" title="账号ID"/>
        <varchar name="name" length="50" title="玩家名称"/>
        <varchar name="accName" length="64" title="账号名称"/>
        <int name="gsId" title="区服ID"/>
        <int name="gsIdx" title="分服下标"/>
        <int name="sdkId" title="SDKID"/>
        <int name="chnId" title="渠道ID"/>
        <varchar name="ico" length="512" title="头像"/>
        <int name="sex" title="性别"/>
        <int name="lvl" title="身份等级"/>
        <int name="vip" title="VIP等级"/>
        <int name="dmd" title="钻石"/>
        <int name="gld" title="金币"/>
        <int name="cmt" title="总属性"/>
        <int name="fmlId" title="商会ID"/>
        <int name="ms" title="查询时间戳"/>
        <datetime name="lTime" title="登陆时间"/>
        <int name="lOsType" title="登录时候的设备系统类型"/>
        <varchar name="inviteCode" length="32" title="邀请好友的激活码"/>
        <datetime name="cTime" title="创建时间"/>
    </table>

    <table name="g_gs_act_status" primary="batchId,grpId">
        <comment>区服活动的处理状态</comment>
        <int name="batchId" title="批次ID"/>
        <int name="grpId" title="区服ID"/>
        <int name="status" title="当前状态"/>
        <datetime name="cTime" title="创建时间"/>
    </table>

    <table name="g_gs_act_rank_hist" primary="uid,batchId">
        <comment>区服活动排行历史记录(保存前20名)</comment>
        <int name="uid" title="玩家ID"/>
        <int name="batchId" title="批次ID"/>
        <int name="grpId" title="区服ID"/>
        <varchar name="name" length="32" title="玩家名字"/>
        <int name="lvl" title="玩家等级"/>
        <int name="vip" title="玩家VIP"/>
        <int name="rank" title="排名"/>
        <int name="rankValue" title="排行值"/>
        <datetime name="cTime" title="创建时间"/>
    </table>

    <table name="g_cp_invite_frds" primary="uid" keys="iUid;accId">
        <comment>Cp好友邀请列表</comment>
        <int name="uid" title="玩家ID"/>
        <int name="accId" title="玩家账号ID"/>
        <int name="iUid" title="邀请者玩家ID"/>
        <int name="iAccId" title="邀请者玩家账号ID"/>

        <varchar name="name" length="32" title="玩家名字"/>
        <int name="lvl" title="玩家等级"/>
        <int name="vip" title="玩家VIP等级"/>
        <varchar name="ico" length="512" title="玩家头像"/>
        <varchar name="thirdIco" length="512" title="玩家第三方头像"/>

        <int name="way" default="1" title="邀请来源">
            <combo>
                <opt name="share" value="1">分享邀请</opt>
                <opt name="code" value="2">邀请码邀请</opt>
                <opt name="jingQi" value="3">晶绮邀请</opt>
            </combo>
        </int>

        <datetime name="uTime" title="更新时间"/>
        <datetime name="cTime" title="创建时间"/>
    </table>

    <table name="g_rchg_req" primary="orderNo" keys="transId;uid;sdkId">
        <comment>充值请求</comment>
        <varchar name="orderNo" length="128" title="游戏订单号"/>
        <varchar name="transId" length="128" title="平台业务号"/>
        <int name="uid" title="玩家ID"/>
        <int name="rid" title="充值项ID"/>
        <int name="rid_pay" title="实际充值项ID">目的是为了做折扣模式的时候，在实际支付是会切换为其他的充值项的需求</int>
        <int name="cny" title="实际支付的cny值">在折扣模式下的时候，这个值会按照实际充值时候的折扣充值项的来走</int>
        <int name="status" title="状态">
            <comment>三种状态分别对应reqTime、sdkTime和sendTime</comment>
            <combo>
                <opt name="req" value="0">拉起请求</opt>
                <opt name="sdk" value="1">待发货</opt>
                <opt name="send" value="2">已发货</opt>
            </combo>
        </int>
        <int name="sdkId" required="true"/>
        <int name="buyType" required="true" title="购买类型">
            <combo>
                <opt name="normal" value="0">自充值</opt>
                <opt name="toDst" value="10">为目标玩家充值</opt>
                <opt name="fromDst" value="11">被目标玩家充值</opt>
            </combo>
        </int>
        <int name="dstUid" title="目标用户id">buyType为normal的时候值为0，toDst和fromDst的时候为目标用户的id</int>
        <int name="type" required="true" title="充值类型">类型见CONST.rchgMode。</int>
        <int name="actBatchId" title="活动批次id">例如type为活动类型时有用，记录充值的活动批次Id</int>
        <varchar name="value" length="64">type指定模式下对应的值，例如type为活动类型，那么这个值表示的就是活动的id</varchar>
        <varchar name="ip" length="128" title="支付IP"/>
        <text name="snapshot" title="快照">用户拉起充值的时候的快照信息，例如vip是多少级等</text>
        <text name="cbInfo" title="充值回调信息"/>
        <text name="err" title="错误信息"/>
        <text name="ext" type="any" title="扩展参数"/>
        <datetime name="reqTime" title="用户拉起充值请求的时间"/>
        <datetime name="sdkTime" title="cp接收到平台的支付回调的时间"/>
        <datetime name="sendTime" title="游戏进行发货的时间"/>
        <int name="isRefund" title="是否已退款"/>
    </table>

    <table name="g_only_name" primary="type,typeId" keys="name;type">
        <comment>名字唯一表</comment>
        <int name="type" title="类型">
            <combo>
                <opt name="fmlName" value="1">家族名称</opt>
            </combo>
        </int>
        <int name="typeId" title="类型对应的ID">例:家族对应的就是家族ID</int>
        <varchar name="name" length="32" title="名字"/>
        <int name="status" default="1" title="状态">
            <combo>
                <opt name="disabled" value="0">失效</opt>
                <opt name="enabled" value="1">生效</opt>
            </combo>
        </int>
    </table>

    <table name="g_custom_ico" primary="md5">
        <comment>玩家自定义头像md5与base64映射表</comment>
        <varchar name="md5" length="32" title="MD5值"/>
        <text name="imgText" length="base64值"/>
    </table>

    <table name="g_act_tmp" primary="id">
        <comment>活动模板表</comment>
        <int name="id" title="模板id">id做了约束,通过规则生成</int>
        <varchar name="name" length="64" title="模板名称"/>
        <varchar name="desc" length="64" title="模板简要描述"/>
        <int name="type" required="true" title="活动类型">
            <combo>
                <!--系统活动:01-19 -->
                <opt name="fashionShop" value="1">时装商城[占用]</opt>
                <opt name="kingOrder" value="2">通行证[占用]</opt>

                <!--充值活动:20-49-->
                <opt name="rchg_sum" value="20">累计充值</opt>
                <opt name="rchg_sum_d" value="21">每日充值</opt>
                <opt name="rchg_days" value="22">累天充值</opt>
                <opt name="rchg_dgift" value="23">每日礼包</opt>
                <opt name="rchg_mine" value="24">海岛礼包</opt>
                <opt name="rchg_rune" value="25">符石礼包[废弃]</opt>
                <opt name="rchg_popBag" value="26">弹窗礼包</opt>
                <opt name="rchg_popBag2" value="27">新弹窗礼包</opt>
                <opt name="rchg_maze" value="28">迷宫礼包</opt>

                <!--特惠礼包:50-59 -->
                <opt name="spclGift" value="50">特惠礼包</opt>

                <!-- 冲榜活动:60-90-->
                <opt name="list_usrLvl" value="60">玩家等级冲榜</opt>
                <opt name="list_runeCoin" value="61">符石币冲榜</opt>

                <!--运营活动:100-xxx-->
                <opt name="lTask" value="100">限时活动任务</opt>
                <opt name="egg" value="101">砸金蛋</opt>
                <opt name="lottery" value="102">欢乐抽奖机</opt>
                <opt name="redPacket" value="103">红包雨</opt>
                <opt name="memory" value="104">记忆大师</opt>


            </combo>
        </int>
        <varchar name="cnd" type="any" length="512" title="模板条件"/>
        <varchar name="imgIco" length="128" title="活动图标"/>
        <varchar name="imgTitle" length="128" title="活动标题Banner图"/>
        <varchar name="imgBg" length="128" title="活动背景图"/>
        <text name="giftbag" type="any[]" title="特惠礼包">格式:[{id:,name:,dmd:,},...]</text>
        <text name="content" type="any[]" title="达标奖励">格式:[[gain,cnd,ext],...]</text>
        <text name="rank" type="any" title="排行奖励">格式：{"usr:":[],"fml":[]}</text>
        <text name="rank2" type="any" title="二阶段排行奖励">格式：{"usr:":[],"fml":[]}</text>
        <text name="task" type="any[]" title="任务奖励">格式:[[type,[[id,reward,tarValue],...]],...]</text>
        <text name="buy" type="any[]" title="活动商店">格式:[{},...]</text>
        <varchar name="sItemIds" length="128" type="number[]" title="开放显示的物品列表"/>
        <varchar name="ext" type="any" length="10240" title="拓展字段" :onlyValidJson="true"/>
        <varchar name="langMap" length="2" type="{[lang:string]:any}" title="多语言模板映射"/>
        <datetime name="uTime" title="更新时间"/>
        <datetime name="cTime" title="创建时间"/>
    </table>

    <table name="g_act_tmp_lang" primary="tmpId,lang">
        <comment>活动规则表(多语言)</comment>
        <int name="tmpId" length="11" required="true" title="活动模板Id"/>
        <varchar name="lang" length="16" required="true" title="语言">
            <combo>
                <opt name="zh" value="zh">中文</opt>
                <opt name="ft" value="ft">繁体</opt>
                <opt name="zh_sg" value="zh_sg">中文(海外)</opt>
                <opt name="en" value="en">英文</opt>
                <opt name="jp" value="jp">日语</opt>
                <opt name="kor" value="kor">韩语</opt>
                <opt name="th" value="th">泰语</opt>
                <opt name="vie" value="vie">越南语</opt>
                <opt name="idn" value="idn">印尼语</opt>
                <opt name="msa" value="msa">马来西亚语</opt>
                <opt name="spa" value="spa">西班牙语</opt>
                <opt name="de" value="de">德语</opt>
                <opt name="fra" value="fra">法语</opt>
                <opt name="pt" value="pt">葡萄牙语</opt>
                <opt name="it" value="it">意大利语</opt>
                <opt name="ru" value="ru">俄语</opt>
                <opt name="tr" value="tr">土耳其语</opt>
            </combo>
        </varchar>
        <varchar name="name" length="64" title="模板名称"/>
        <varchar name="rule" length="1024" title="活动规则描述"/>
        <varchar name="textMap" length="4096" type="any" title="文本映射"/>
        <datetime name="uTime" title="更新时间"/>
        <datetime name="cTime" title="创建时间"/>
    </table>

    <table name="g_act_batch" primary="id">
        <comment>活动批次表</comment>
        <int name="id" auto="true" title="批次ID"/>
        <varchar name="name" length="64" title="活动名称"/>
        <varchar name="desc" length="64" title="活动描述"/>
        <int name="tmpId" required="true" title="模板ID"/>
        <int name="tmpType" length="11" title="模板类型"/>
        <int name="status" title="状态">
            <combo>
                <opt name="disabled" value="0">失效</opt>
                <opt name="enabled" value="1">生效</opt>
            </combo>
        </int>
        <int name="effType" title="生效类型">
            <comment>0表示绝对时间,即使用beginTime字段;1表示根据开服时间,即使用beginDay字段;2表示根据创角时间，即使用beginDay字段;3表示根据合服时间，即使用beginDay字段</comment>
            <combo>
                <opt name="absoluteTime" value="0">绝对时间</opt>
                <opt name="gsOpenTime" value="1">开服时间</opt>
                <opt name="usrCreateTime" value="2">创角时间</opt>
                <opt name="gsMergerTime" value="3">合服时间</opt>
            </combo>
        </int>
        <datetime name="beginTime" title="开始时间">对应effType=0</datetime>
        <varchar name="beginDay" length="16" title="开服几天后生效">对应effType=1或者2,配置的表达式为: 天数,小时数,分钟数 或者: 天数,小时数 或者: 天数.</varchar>
        <varchar name="duration" length="16" title="活动持续的时间">配置的表达式为: 天数,小时数,分钟数 或者: 天数,小时数 或者: 天数.</varchar>
        <varchar name="duration_before" length="16" title="活动开始前显示持续的时间">配置的表达式和duration一致</varchar>
        <varchar name="duration_after" length="16" title="活动结束后显示持续的时间">配置的表达式和duration一致</varchar>
        <int name="sort" title="排序(越小越前面)"/>
        <int name="noticeId" default="0" title="公告模板ID"/>
        <varchar name="gsIdList_white" length="1024" type="string[]" title="分服白名单">格式：[1,"1-2", "2:*"]</varchar>
        <varchar name="gsIdList_black" length="1024" type="string[]" title="分服黑名单">格式：[1,"1-2", "2:*"]</varchar>
        <text name="roomMap" type="any" title="跨服分组映射"/>
        <datetime name="uTime" title="更新时间"/>
        <datetime name="cTime" title="创建时间"/>
    </table>

    <table name="g_act_grp" primary="id">
        <comment>活动分服表</comment>
        <int name="id" auto="true" title="主键ID"/>
        <varchar name="grpIdList" length="2096" type="number[]" title="开启的区服列表">这里不做黑白名单，一般是对几个指定区服开启</varchar>
        <int name="batchId" required="true" title="活动批次ID"/>
        <varchar name="name" length="64" title="活动名称"/>
        <varchar name="desc" length="64" title="活动描述"/>
        <int name="tmpId" required="true" title="模板ID"/>
        <int name="status" title="状态">
            <combo>
                <opt name="disabled" value="0">失效</opt>
                <opt name="enabled" value="1">生效</opt>
            </combo>
        </int>
        <int name="effType" title="生效类型">
            <comment>0表示绝对时间,即使用beginTime字段;1表示根据开服时间,即使用beginDay字段;2表示根据创角时间，即使用beginDay字段</comment>
            <combo>
                <opt name="absoluteTime" value="0">绝对时间</opt>
                <opt name="gsOpenTime" value="1">开服时间</opt>
                <opt name="usrCreateTime" value="2">创角时间</opt>
            </combo>
        </int>
        <datetime name="beginTime" title="开始时间">对应effType=0</datetime>
        <varchar name="beginDay" length="16" title="开服几天后生效">同g_act_batch表</varchar>
        <varchar name="duration" length="16" title="活动持续的时间">同g_act_batch表</varchar>
        <varchar name="duration_before" length="16" title="活动开始前显示持续的时间">同g_act_batch表</varchar>
        <varchar name="duration_after" length="16" title="活动结束后显示持续的时间">同g_act_batch表</varchar>
        <int name="sort" title="排序(越小越前面)"/>
        <int name="noticeId" default="0" title="公告模板ID"/>
        <datetime name="uTime" title="更新时间">记录的是GM更新的时候的更新时间</datetime>
    </table>

    <table name="g_act_notice" primary="id">
        <comment>活动公告信息</comment>
        <int name="id" auto="true" title="公告ID"/>
        <varchar name="name" length="50" title="公告名字(无用运营看)"/>
        <int name="mainType" title="对应的活动主类型">额外字段，避免要建结构体</int>
        <varchar name="ttl" length="50" title="活动标题"/>
        <text name="content" title="活动内容"/>
        <varchar name="img" length="1024" title="内容图片"/>
        <varchar name="img_frm" length="1024" title="图片边框"/>
        <int name="isHot" default="0" title="是否是热门活动">
            <combo>
                <opt name="no" value="0">否</opt>
                <opt name="yes" value="1">是</opt>
            </combo>
        </int>
        <int name="isTurns" title="是否轮播">
            <combo>
                <opt name="no" value="0">否</opt>
                <opt name="yes" value="1">是</opt>
            </combo>
        </int>
        <int name="sort" title="排序(越大越前面)"/>
        <int name="cd" title="剩余CD">额外字段，避免要建结构体</int>
        <int name="isNew" title="是否是新公告">额外字段，避免要建结构体</int>
        <varchar name="param" type="any" length="32" title="参数">额外字段，避免要建结构体</varchar>
        <datetime name="uTime" title="更新时间"/>
        <datetime name="cTime" title="创建时间"/>
    </table>

    <table name="g_act_notice_lang" primary="tmpId,lang">
        <comment>活动公告信息-多语言</comment>
        <int name="tmpId" default="0">m_notice中的id</int>
        <varchar name="lang" length="12" title="语言类型">
            <combo>
                <opt name="zh" value="zh">中文</opt>
                <opt name="ft" value="ft">繁体</opt>
                <opt name="zh_sg" value="zh_sg">中文(海外)</opt>
                <opt name="en" value="en">英文</opt>
                <opt name="jp" value="jp">日语</opt>
                <opt name="kor" value="kor">韩语</opt>
                <opt name="th" value="th">泰语</opt>
                <opt name="vie" value="vie">越南语</opt>
                <opt name="idn" value="idn">印尼语</opt>
                <opt name="msa" value="msa">马来西亚语</opt>
                <opt name="spa" value="spa">西班牙语</opt>
                <opt name="de" value="de">德语</opt>
                <opt name="fra" value="fra">法语</opt>
                <opt name="pt" value="pt">葡萄牙语</opt>
                <opt name="it" value="it">意大利语</opt>
                <opt name="ru" value="ru">俄语</opt>
                <opt name="tr" value="tr">土耳其语</opt>
            </combo>
        </varchar>
        <varchar name="ttl" length="50" title="活动标题"/>
        <text name="content" title="活动内容"/>
