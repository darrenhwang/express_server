    <table name="g_script_cfg" primary="ver,subVer,func">
        <comment>脚本执行配置</comment>
        <varchar name="ver" length="64" title="版本号[x.x.x]"/>
        <int name="subVer" title="子版本号">从0开始,用户定义</int>
        <varchar name="func" length="64" title="脚本函数"/>
        <varchar name="gsList" length="1024" type="string[]" title="配置区服列表">Fgs配置模式</varchar>
        <text name="params" type="any" length="512" title="脚本执行需要的额外参数">由个脚本自定义</text>
        <varchar name="desc" length="1024" title="脚本内容描述"/>
        <int name="status">
            <comment>脚本状态</comment>
            <combo>
                <opt name="enable" value="1">有效</opt>
                <opt name="disable" value="0">无效</opt>
            </combo>
        </int>
        <datetime name="cTime" title="创建时间"/>
    </table>

    <table name="g_script_progress" primary="uid,ver,subVer,func,db">
        <comment>脚本执行进度表(记录某个玩家是否已执行过脚本)</comment>
        <int name="uid" title="玩家ID或表的主键ID"/>
        <varchar name="ver" length="64" title="版本号[x.x.x]"/>
        <int name="subVer" title="子版本号">从0开始,用户定义</int>
        <varchar name="func" length="64" title="脚本函数"/>
        <varchar name="db" length="512" title="DB链接信息"/>
        <int name="grpId" title="区服ID"/>
        <varchar name="content" length="2096" title="执行内容信息"/>
        <datetime name="cTime" title="创建时间"/>
    </table>

    <table name="g_script_version" primary="ver,subVer,func,db">
        <comment>脚本执行版本记录表</comment>
        <varchar name="ver" length="64" title="版本号[x.x.x]"/>
        <int name="subVer" title="子版本号">从0开始,用户定义</int>
        <varchar name="func" length="64" title="脚本函数"/>
        <varchar name="db" length="512" title="DB链接信息"/>
        <int name="grpId" title="区服ID"/>
        <int name="status" title="当前状态">
            <comment>当前状态</comment>
            <combo>
                <opt name="no" value="0">未执行</opt>
                <opt name="yes" value="1">已执行</opt>
            </combo>
        </int>
        <text name="progress" type="any[]" title="执行进度"/>
        <datetime name="cTime" title="创建时间"/>
    </table>

    <table name="g_share_cfg" primary="id">
        <comment>分享配置</comment>
        <int name="id" title="分享ID(同c_share.xlsx的ID)"/>
        <varchar name="name" length="64" title="分享名字"/>
        <int name="hasShare" title="是否有分享">
            <combo>
                <opt name="no" value="0">无</opt>
                <opt name="yes" value="1">有</opt>
            </combo>
        </int>
        <int name="hasVideo" title="是否有视频">
            <combo>
                <opt name="no" value="0">无</opt>
                <opt name="yes" value="1">有</opt>
            </combo>
        </int>
        <varchar name="chnIdList" length="256" type="any[]" title="渠道列表"/>
        <varchar name="osTypeList" length="256" type="any[]" title="系统列表"/>
        <varchar name="chgVideo" length="256" type="any" title="功能转换配置"/>
        <datetime name="uTime" title="更新时间"/>
        <datetime name="cTime" title="创建时间"/>
    </table>

    <table name="g_share_tmp" primary="id">
        <comment>分享模板信息</comment>
        <int name="id" auto="true" title="主键ID"/>
        <int name="type" title="分享类型">
            <combo>
                <opt name="daily" value="1">每日分享</opt>
                <opt name="group" value="2">群分享</opt>
                <opt name="invite" value="3">邀请好友</opt>
            </combo>
        </int>
        <varchar name="shareTitle" length="256" title="分享标题"/>
        <varchar name="shareDesc" length="1024" title="分享描述"/>
        <varchar name="shareImg" length="256" title="分享图片链接"/>
        <datetime name="uTime" title="更新时间"/>
        <datetime name="cTime" title="创建时间"/>
    </table>

    <table name="g_redeem_acc" primary="aid,useCode">
        <comment>账号使用兑换码信息</comment>
        <int name="aid" title="账号ID"/>
        <varchar name="useCode" length="32" title="使用的兑换码"/>
        <datetime name="useTime" title="使用时间"/>
    </table>

    <table name="g_redeem_code" entity="IRedeemCode" primary="id">
        <comment>兑换码使用情况</comment>
        <int name="id" auto="true" title="主键ID"/>
        <varchar name="code" length="32" title="兑换码"/>
        <varchar name="batchCode" required="true" length="32" title="兑换批次码"/>
        <int name="aid" title="账号Id，如果有兑换，则有值"/>
        <int name="uid" title="角色Id，如果有兑换，则有值"/>
        <datetime name="uTime" title="更新时间"/>
        <datetime name="cTime" title="创建时间"/>
    </table>

    <table name="g_redeem_batch" entity="IRedeemBatch" primary="code">
        <comment>兑换码批次表</comment>
        <varchar name="code" length="32" title="批次码"/>
        <varchar name="name" length="64" required="true" title="名称">目的是为了方便维护，游戏内容没有实际用到</varchar>
        <int name="status" default="1" title="是否有效">
            <combo>
                <opt name="enable" value="1">有效</opt>
                <opt name="disable" value="0">失效</opt>
            </combo>
        </int>
        <int name="type" required="true" default="1" title="类型">
            <combo>
                <opt name="codeOnce" value="1">重复激活</opt>
                <opt name="accMakeCode" value="2">账号单次激活通码</opt>
                <opt name="usrMakeCode" value="3">角色单次激活通码</opt>
                <opt name="usrOnce" value="4">角色单次激活</opt>
                <opt name="accOnce" value="5">账号单次激活(超甜使用)</opt>
            </combo>
        </int>
        <varchar name="sdkBatch" length="64" title="社区兑换码批次"/>
        <varchar name="sdkBatchKey" length="64" title="社区兑换批次Key"/>
        <int name="lvl" default="0" title="角色等级限制"/>
        <int name="vip" default="0" title="VIP等级限制"/>
        <varchar name="gsList" length="200" type="any[]" title="分服列表"/>
        <text name="chnIdList" length="256" type="any[]" title="渠道列表"/>
        <text name="chnIdList_black" length="256" type="any[]" title="渠道黑名单列表"/>
        <int name="dailyLimit" default="0" title="是否有每日限制">
            <combo>
                <opt name="no" value="0">没有每日限制</opt>
                <opt name="yes" value="1">每日限制兑换一次</opt>
            </combo>
        </int>
        <varchar name="items" length="512" title="奖励内容" required="true" :vue-type="items">格式为：1,2000;2,10000</varchar>
        <varchar name="content" length="256" title="内容说明"/>
        <int name="mailId" default="0" title="领奖邮件ID"/>
        <datetime name="startTime" title="开始时间"/>
        <datetime name="endTime" title="结束时间"/>
        <int name="applyGmUserId" title="发起人id"/>
        <varchar name="applyGmUserName" length="50" title="发起人名称"/>
        <int name="reviewerId" title="审核人id"/>
        <varchar name="reviewerName" length="50" title="审核人名称"/>
        <int name="isCheck" default="0" title="是否审核">
            <combo>
                <opt name="yes" value="1">已审核</opt>
                <opt name="no" value="0">未审核</opt>
            </combo>
        </int>
        <text name="fail" title="失败记录"/>
        <datetime name="cTime" title="创建时间"/>
    </table>

    <table name="g_gift_acc" primary="batchId,aid" keys="batchId;aid">
        <comment>礼包帐号领取表</comment>
        <int name="batchId" title="批次ID"/>
        <int name="aid" title="账号ID"/>
        <int name="uid" title="玩家ID"/>
        <int name="gsIdx" title="分服索引"/>
        <datetime name="uTime" title="更新时间"/>
        <datetime name="cTime" title="创建时间"/>
    </table>

    <table name="g_gift_batch" primary="id" keys="id">
        <comment>礼包批次表</comment>
        <int name="id" default="0" title="类型"/>
        <varchar name="name" length="64" title="名称(运营看)"/>
        <int name="type" title="礼包类型">
            <combo>
                <opt name="qq" value="0">QQ礼包</opt>
                <opt name="bonbon" value="1">社区礼包</opt>
            </combo>
        </int>
        <varchar name="gsIdxList" length="200" type="number[]" title="分服索引列表"/>
        <datetime name="sTime" title="生效时间"/>
        <datetime name="eTime" title="失效时间"/>
        <varchar name="items" length="512" title="奖励内容">格式为：1,2000;2,10000</varchar>
        <varchar name="chnIdList" length="256" type="any[]" title="渠道列表"/>
        <int name="vip" title="VIP等级限制"/>
        <int name="lvl" title="角色等级限制"/>
        <int name="cycleType" title="礼包周期类型"/>
        <int name="thirdVip" title="第三方的VIP等级限制"/>
        <int name="status" default="1" title="礼包状态">
            <combo>
                <opt name="no" value="0">无效</opt>
                <opt name="yes" value="1">有效</opt>
            </combo>
        </int>
    </table>

    <table name="g_cron_log" primary="id">
        <comment>定时器执行日志</comment>
        <int name="id" auto="true" title="主键ID"/>
        <int name="grpId" title="区服Id"/>
        <int name="type" title="类型">
            <combo>
                <opt name="mineGoodBrush" value="1">矿场刷出</opt>
                <opt name="mineRockBrush" value="2">矿洞刷出</opt>
            </combo>
        </int>
        <int name="ms" title="执行秒数"/>
        <varchar name="data" length="1024" type="any" title="数据"/>
        <datetime name="uTime" title="更新时间"/>
        <datetime name="cTime" title="创建时间"/>
    </table>

    <table name="g_reported_usr" primary="type">
        <comment>被举报的玩家列表</comment>
        <int name="type" title="类型">
            <combo>
                <opt name="mine" value="1">海岛</opt>
            </combo>
        </int>
        <text name="uidList" type="any[]" title="被举报玩家列表"/>
        <datetime name="uTime" title="更新时间"/>
        <datetime name="cTime" title="创建时间"/>
    </table>

    <table name="g_table_version" primary="grpId">
        <comment>数据库表版本,用来处理洗数据</comment>
        <int name="grpId" title="区服Id"/>
        <int name="ver" title="最新版本">由程序自己定义，非程序的版本</int>
        <text name="hasExecVers" type="number[]" title="已执行版本号"/>
        <datetime name="uTime" title="更新时间"/>
    </table>
</ds>
